{
  "name": "アンチAI過激派くん",
  "description": "AIなんぞ、使ってんじゃねぇ！",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "ANTI.png",
    "48": "ANTI.png",
    "128": "ANTI.png"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset",
        "enabled": true,
        "path": "ruleset.json"
      }
    ]
  },
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://chatgpt.com/*",
    "*://www.perplexity.ai/*",
    "*://claude.ai/*",
    "*://gemini.google.com/*",
    "*://www.midjourney.com/*",
    "*://stability.ai/*",
    "*://dreamstudio.ai/*",
    "*://runwayml.com/*",
    "*://www.jasper.ai/*",
    "*://www.copy.ai/*",
    "*://replika.ai/*",
    "*://bolt.new/*",
    "*://dify.ai/*",
    "*://replit.com/*",
    "*://www.tabnine.com/*"
  ]
}